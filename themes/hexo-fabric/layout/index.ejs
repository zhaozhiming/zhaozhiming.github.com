<%- partial('_partial/disqus', { index: true }) %>
<div class="index-layout">
  <div class="side-bar">
    <aside class="sidebar">
      <% config.default_asides.forEach(function(aside){ %>
        <%- partial(aside) %>
      <% }) %>
    </aside>
  </div>
  <div class="left-content">
    <div class="left-post">
<% page.posts.each(function(post){ %>
      <article class="post"><%- partial('_partial/article', {post: post, index: true }) %></article>
<% }) %>
<% if (page.total > 1) { %>
    <nav id="pagenavi">
    <% if (page.prev) { %>
      <a href="/<%- page.prev_link %>" class="prev">Prev</a>
    <% } %>
    <% if ( page.next ) { %>
      <a href="/<%- page.next_link %>" class="next">Next</a>
    <% } %>
      <div class="center"><a href="/archives/index.html">Blog Archives</a></div>
    </nav>
<% } %>
    </div>
  </div>
</div>
